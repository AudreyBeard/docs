{
  "index_name": "docodile",
  "sitemaps": ["https://docs.wandb.ai/sitemap.xml"],
  "start_urls": ["https://docs.wandb.ai"],
  "stop_urls": ["https://docs.wandb.ai/ja", "https://docs.wandb.ai/ko"],
  "sitemap_alternate_links": false,
  "selectors": {
    "lvl0": {
      "selector": "(//ul[contains(@class,'menu__list')]//a[contains(@class, 'menu__link menu__link--sublist menu__link--active')]/text() | //nav[contains(@class, 'navbar')]//a[contains(@class, 'navbar__link--active')]/text())[last()]",
      "type": "xpath",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": "header h1, main h1",
    "lvl2": "main h2",
    "lvl3": "main h3",
    "lvl4": "main h4",
    "lvl5": "main h5, main td:first-child",
    "lvl6": "main h6",
    "text": "main p, main li, main code, main td:last-child"
  },
  "strip_chars": " .,;:#",
  "custom_settings": {
    "separatorsToIndex": "_",
    "attributesForFaceting": ["language", "version", "type"],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type"
    ]
  }
}
